{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "static",
  "buildCommand": "",
  "outputDirectory": ".",
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
